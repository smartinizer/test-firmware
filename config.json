{
    "name": "LED",
    "firmware": "https://relais.smartinizer.devzero.cloud/firmware.bin",
    "config-schema": [
      {
        "key": "led_pin",
        "description": "The Pin the LED is connected to",
        "format": "^[0-9]*$",
        "default": "2"
      },
      {
        "key": "topic",
        "description": "Specifies the topic that will be subscribed to",
        "format": "^[a-z]+$",
        "default": "command"
      }
    ]
  }
